            "redcap_field_note": "(+0) < 1.2<br>(+1) 1.2 - 1.9<br>(+2) 2.0 - 5.9<br>(+3) 6.0 - 11.9<br>(+4) >= 12.0",
            "redcap_options": "if([tbil_max] != \"\", if([tbil_max] >= 12, 4, if([tbil_max] >= 6,3, if([tbil_max] >= 2, 2, if([tbil_max] >= 1.2, 1, 0)))), \"\")",
